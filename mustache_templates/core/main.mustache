<!doctype html>
<html lang="en" ng-app="app" ng-controller="CoreController as app">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, user-scalable=no">
    <title>Zowcast</title>
    <%# assets %>
        <%# css %>
            <%# remote %><link href="<% . %>" rel='stylesheet' type='text/css'><%/ remote %>
            <%# contrib %><link href="css/contrib/<% . %>.css" rel='stylesheet' type='text/css'> <%/ contrib %>
    <%# libs %><link href="css/libs/<% . %>.css" rel='stylesheet' type='text/css'><%/ libs %>
    <%# staging %><link href="css/styles.min.css" rel='stylesheet' type='text/css'> <%/ staging %>
        <%/ css %>
    <%/ assets %>
</head>

<body >

<player></player>

<div id="stage">

    <span ng-controller="NavigationController">
            <i ng-click="toggleMenu()" class="fa fa-bars menuToggler"></i>
    </span>

    <div ng-if="overlay" class="overlay"></div>

    <overlay></overlay>

    <div ng-if="message" class="swMessage">

        <div class="swText {{message.msgType}}">
            <i ng-click="closeMessage()" class="fa fa-close swClose"></i>
            {{message.text}}
        </div>
    </div>

    <% > ui/navBar %>

    <div ng-view class="swContainer"></div>

</div>

<%# assets %>
    <%# js %>
        <%# remote %>
<script src="<% . %>"></script><br/>
<%/ remote %>
        <%# contrib %>
<script src="src/contrib/<%& . %>.js"></script>
<%/ contrib %>
<%# libs %>
<script src="src/libs/<%& . %>.js"></script>
<%/ libs %>
<%# staging %> <script src="src/main.min.js"></script> <%/ staging %>
    <%/ js %>
<%/ assets %>
</body>

</html>
<!-- End Generated Code: mustache_templates/app/main.mustache -->
